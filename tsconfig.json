{
  "compilerOptions": {
    "target": "es2016",
    "module": "ES2020", 
    "lib": ["ES6"],
    "allowJs": true,
    "outDir": "./build", 
    "rootDir": "./src",
    "strict": true,
    "noImplicitAny": true, 
    "esModuleInterop": true, 
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  },
  "exclude":[
    "./node_modules"
  ]
}
